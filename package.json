    "gulp-less": "4.0.1",
    "gulp-libsquoosh": "^1.0.10",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgstore": "^8.0.0",
