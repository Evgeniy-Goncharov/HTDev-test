{
  "name": "htdev",
  "version": "1.0.0",
  "description": "Тестовое задание от HTDev",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.2.5",
    "browser-sync": "2.26.14",
